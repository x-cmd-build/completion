# shellcheck shell=dash
_completion_shellfile_copy(){
    local l; x fsiter --folder | while read -r l; do
        x:info "Copy $l/* to $___X_CMD_ROOT_DATA/advise/completion/$___X_CMD_ADVISE_MAN_PKG_VERSION/$l-completion"
        command cp "$l/"* "$___X_CMD_ROOT_DATA/advise/completion/$___X_CMD_ADVISE_MAN_PKG_VERSION/$l-completion"
    done
}

# _completion_shellfile_copy
